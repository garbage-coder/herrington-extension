{
	"manifest_version": 3,
	"name": "Herrington Extension",
	"version": "1.3",
	"description": "Replace all images for Herrington!!!",
	"icons": {
	  "16": "images/icon16.png",
	  "48": "images/icon48.png",
	  "128": "images/icon128.png"
	},
	"content_scripts": [
	  {
		"matches": ["*://*/*"],
		"js": ["jquery.js", "herrington.js"]
	  }
	]
  }
  